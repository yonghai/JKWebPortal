<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="jk.o1office.domin.CustomerAddress" table="user_customer_delivery_address" dynamic-update="true">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="customer_id"/>
		<property name="contacts"/>
		<property name="contact_number"/>
		<property name="area" type="java.lang.Integer"/>	
		<property name="community" type="java.lang.Integer" />
		<property name="address"/>
		<property name="is_default"/>
		<property name="communityName" column="communityname"/>
		<property name="countryName" column="countryname"/>
		<property name="provinceName" column="provincename"/>
		<property name="regionalName" column="regionalname"/>
		<property name="createtime" type="timestamp"/>	
    </class>
	
</hibernate-mapping>