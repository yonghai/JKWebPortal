<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="jk.o1office.domin.Complain" table="complain">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="contact"/>
		<property name="content" column="complain_content"/>
		<property name="time" column="complain_time" type="timestamp"/>
		<property name="userid" column="customer_id"/>
    </class>
	
</hibernate-mapping>