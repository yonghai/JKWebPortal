			<tx:method name="service" propagation="REQUIRED" read-only="false" rollback-for="Exception"/>
			<tx:method name="updateInfo" propagation="REQUIRED" read-only="false" rollback-for="Exception"/>
			<!-- DeviceInfoService -->
