			<tx:method name="init" propagation="REQUIRED" read-only="true" rollback-for="Exception"/>
		</tx:attributes>
	</tx:advice>
	<!-- 事务管理 -->
	<aop:config>
		<aop:pointcut expression="execution(* jk.o1office.service.*.*(..))" id="service"/>
		<aop:advisor advice-ref="txAdvice" pointcut-ref="service"/>
	</aop:config>
	
