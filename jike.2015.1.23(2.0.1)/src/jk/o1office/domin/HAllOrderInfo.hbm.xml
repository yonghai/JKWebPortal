<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="jk.o1office.domin.HAllOrderInfo" table="allorder_history">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="orderid" unique="true"/>
		<property name="ordertype"/>
		<property name="customerid"/>
		<property name="deliveryid"/>
		<property name="bill_status"/>
		<property name="isview"/>
		<property name="checkcode"/>
		<property name="deliveryprice"/>
		<property name="total_price"/>
		<property name="goodsnum"/>
		<property name="address"/>
		<property name="addressid"/>
		<property name="tel"/>
		<property name="name"/>
		<property name="createtime" type="timestamp"/>	
    </class>
	
</hibernate-mapping>