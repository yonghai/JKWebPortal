<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="jk.o1office.domin.DeviceInfo" table="device_model">
		<id name="id" column="_id">
			<generator class="native"></generator>
		</id>
		<property name="device_id" column="device_id"/>
		<property name="device_name"/>
		<property name="device_type"/>
		<property name="device_resolution"/>
		<property name="device_screen"/>
		<property name="device_os"/>
		<property name="device_os_version"/>
		<property name="device_hardware_vendors"/>
		<property name="device_detail_info"/>
		<property name="device_mac"/>	
		<property name="createTime" type="timestamp"/>	
    </class>
	
</hibernate-mapping>