<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="jk.o1office.domin.GoodsCategory" table="product_category">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="goodsname" column="category_name"/>
		<property name="goodsimg" column="category_img"/>
		<!-- property name="level" column="level"/-->
		<property name="version" column="version"/>	
		<property name="pid" column="parent_id"/>
		<property name="descript" column="descript"/>
    </class>
	
</hibernate-mapping>